/* pages/report/report.wxss */

.boxs {
  width: 100%;
  height: 100%;
  position: absolute;
}
.box1 {
  width: 100%;
  height: 100%;
  position: absolute;
  top:0
}

.image{
  width: 100%;
}

.bottom_icon{
  position: absolute;
  bottom:40rpx;
  display: flex;
  width: 100%;
  justify-content: center;
  z-index: 100;
}

.box2 {
  width: 100%;
  height: 100%;
  background: #00A6E4;
  position: absolute;
  top: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0.6;
  color: white;
  flex-direction: column;
}

.box_item{
  padding: 30rpx 80rpx;
}
.box3 {
  width: 100%;
  height: 100%;
  background: #00A6E4;
  position: absolute;
  top: 200%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  color: white;
  opacity: 0.7
}
.box4 {
  width: 100%;
  height: 100%;
  background: #00A6E4;
  position: absolute;
  top: 300%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  color: white;
  opacity: 0.8;
  font-size: 30rpx;
}
.table {
  border: 1px solid white;
  width: 90%
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: rgb(167, 55, 55);
}
.bg-g{
  background:#e5ffff;
  color: #808080
}
.th {
  width: 40%;
  justify-content: center;
  background: #00A6E4;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.box5 {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 400%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #999;
  flex-direction: column
}

.tips{
  color:#00A6E4
}