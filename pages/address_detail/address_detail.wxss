/* pages/address_detail/address_detail.wxss */
page{
  height: 100%;
  width:100%;
}
.page{
  height: 100%;
  width: 100%;
  background: #f2f2f2
}
.edit_content{
  background: white
}
.edit_item{
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  margin-left: 30rpx;
  padding:20rpx 20rpx 20rpx 0;
  border-bottom: 1rpx solid #f2f2f2
}
.edit_name{
  display: flex;
  flex: 1;
}
.edit_value{
  display: flex;
  flex: 3;
  align-items: center;
}
.blank_30{
  height: 20rpx;
  background: #f2f2f2;
}
.default_address{
  background: white;
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  padding:30rpx;
  border-bottom: 1rpx solid #f2f2f2
}
.default_left{
  display: flex;
  flex: 3;
  flex-direction: column;
}
.left_title{
  padding: 10rpx 0rpx;
  font-size: 32rpx;
}
.left_tips{
  font-size: 28rpx;
}
.default_right{
  display: flex;
  flex: 1;
  justify-content: flex-end;
  align-items: center;
}
.add_btn{
  position: fixed;
  bottom: 30px;
  left: 0;
  right: 0;
  z-index: 50;
}
.delete{
  background: white;
  padding: 20rpx 30rpx;
  color: red;
  font-size: 30rpx;
}